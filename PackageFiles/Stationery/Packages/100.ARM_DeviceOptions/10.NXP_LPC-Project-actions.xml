<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!DOCTYPE root SYSTEM "../package.dtd">

<root version="1.0" >
<!-- 
   NXP - LPC
 -->    

   <!-- Base CortexM0 files -->
   <projectActionList id="10.NXP_LPC-Project-actions" >
      <applyWhen>
         <and>
            <deviceFamilyMatches>LPC.*</deviceFamilyMatches>
            <variableRef idRef="outputType" condition="matches" value=".*\.newProjectType\.exe$" />
         </and>
      </applyWhen>
      <!-- not actually used -->
      <constant id="eraseMethod"       value="ERASE_SELECTIVE" />
   </projectActionList>

</root>