<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!DOCTYPE root SYSTEM "../package.dtd">

<root version="1.0">
   <!-- Extra Optional files for MKE02M2, MKE02M4  -->
   <projectActionList id="Options-file-list-MKE02Z4">
      <applyWhen>
         <and>
	    <or>
               <deviceSubfamilyIs>MKE02Z2</deviceSubfamilyIs>
               <deviceSubfamilyIs>MKE02Z4</deviceSubfamilyIs>
	    </or>
            <variableRef idRef="outputType" condition="matches" value=".*ProjectType\.exe$" />
         </and>
      </applyWhen>
      <!-- Clock no support for Internal Clock Source (ICS) -->
      <!-- Security -->
      <copy source="Startup_Code/security-mke02.c"      target="Startup_Code/security.c" />
      <!-- Console -->
      <copy source="Startup_Code/console-mke.c" target="Startup_Code/console.c" />
      <copy source="Project_Headers/console.h"  target="Project_Headers/console.h" />
   </projectActionList>
</root>