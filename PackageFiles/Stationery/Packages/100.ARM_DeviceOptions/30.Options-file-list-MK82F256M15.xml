<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!DOCTYPE root SYSTEM "../package.dtd">

<root version="1.0">
   <!-- Extra Optional files for MK82F256M15 with USB derived Clock -->
   <projectActionList id="Options-file-list-MK82F25615">
      <applyWhen>
         <and>
            <deviceSubfamilyIs>MK82F25615</deviceSubfamilyIs>
            <variableRef idRef="outputType" condition="matches" value=".*ProjectType\.exe$" />
         </and>
      </applyWhen>
      <!-- Security with boot option -->
      <copy source="Startup_Code/security-mk-boot.c"    target="Startup_Code/security.c" />
      <!-- Clock -->
      <copy source="Startup_Code/clock-MCG-MKL82.c"              target="Startup_Code/clock.c" />
      <copy source="Project_Headers/clock_configure-MCG-MK82F.h" target="Project_Headers/clock_configure.h" />
      <!-- Console -->
      <copy source="Startup_Code/console-lpuart.c" target="Startup_Code/console.c" />
      <copy source="Project_Headers/console.h"     target="Project_Headers/console.h" />
   </projectActionList>
</root>