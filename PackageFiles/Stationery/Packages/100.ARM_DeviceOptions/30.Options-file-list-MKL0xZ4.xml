<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!DOCTYPE root SYSTEM "../package.dtd">

<root version="1.0">
   <!-- Extra Optional files for MKL0x -->
   <projectActionList id="Options-file-list-MKL0xZ4">
      <applyWhen>
         <and>
            <or>
               <deviceSubfamilyIs>MKL02Z4</deviceSubfamilyIs>
               <deviceSubfamilyIs>MKL04Z4</deviceSubfamilyIs>
               <deviceSubfamilyIs>MKL05Z4</deviceSubfamilyIs>
            </or>
            <variableRef idRef="outputType" condition="matches" value=".*ProjectType\.exe$" />
         </and>
      </applyWhen>
      <!-- Clock -->
      <copy source="Startup_Code/clock-MCG-MKL0x.c"              target="Startup_Code/clock.c" />
      <copy source="Project_Headers/clock_configure-MCG-MKL0x.h" target="Project_Headers/clock_configure.h" />
      <!-- Console -->
      <copy source="Startup_Code/console-mkl.c" target="Startup_Code/console.c" />
      <copy source="Project_Headers/console.h"  target="Project_Headers/console.h" />
   </projectActionList>
</root>